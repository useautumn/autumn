						"group": "Products",
						"pages": [
							"api-reference/products/get-product",
							"api-reference/products/list-products",
							"api-reference/products/create-product",
							"api-reference/products/update-product",
							"api-reference/products/delete-product"
						]
					},
					{
						"group": "Referrals",
						"pages": [
							"api-reference/referrals/referral-code",
							"api-reference/referrals/referral-redeem"
						]
					},

					{
						"group": "CLI",
						"pages": ["api-reference/cli/config"]
					},
					{
						"group": "Platform (Beta)",
						"pages": [
							"api-reference/platform/overview",
							"api-reference/platform/create-organization",
							"api-reference/platform/oauth-url",
							"api-reference/platform/update-stripe",
							"api-reference/platform/list-users",
							"api-reference/platform/list-orgs"
