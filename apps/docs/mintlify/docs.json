		"playground": {
			"display": "interactive"
		},
		"examples": {
			"languages": ["typescript", "bash", "python"]
		}
	},
	"background": {
		"color": {
			"light": "#FAFAFA",
			"dark": "#121212"
		}
	},
	"navbar": {
		"primary": {
			"type": "button",
			"label": "Dashboard",
			"href": "https://app.useautumn.com"
		}
	},
	"footer": {
		"socials": {
			"x": "https://x.com/autumnpricing",
